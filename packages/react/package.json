{
  "name": "@incubateur-ademe/legal-pages-react",
  "version": "0.0.3-next.1",
  "description": "Legal pages as components for React",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/cjs/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js",
      "node": "./dist/cjs/index.js",
      "types": "./dist/index.d.ts"
    },
    "./CookiesPolicy": {
      "import": "./dist/es/CookiesPolicy.js",
      "require": "./dist/cjs/CookiesPolicy.js",
      "node": "./dist/cjs/CookiesPolicy.js",
      "types": "./dist/CookiesPolicy.d.ts"
    },
    "./LegalNotice": {
      "import": "./dist/es/LegalNotice.js",
      "require": "./dist/cjs/LegalNotice.js",
      "node": "./dist/cjs/LegalNotice.js",
      "types": "./dist/LegalNotice.d.ts"
    },
    "./PrivacyPolicy": {
      "import": "./dist/es/PrivacyPolicy.js",
      "require": "./dist/cjs/PrivacyPolicy.js",
      "node": "./dist/cjs/PrivacyPolicy.js",
      "types": "./dist/PrivacyPolicy.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/incubateur-ademe/legal-pages.git",
    "directory": "packages/react"
  },
  "author": "Incubateur ADEME <accelerateurdelatransitionecologique@ademe.fr>",
  "license": "MIT",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite build --watch",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "peerDependencies": {
    "react": "^18.3.0",
    "react-dom": "^18.3.0"
  },
  "devDependencies": {
    "@tsconfig/vite-react": "^3.0.2",
    "@types/dompurify": "^3.0.5",
    "@types/mustache": "^4.2.5",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint-plugin-react": "^7.34.1",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "rollup-plugin-preserve-directives": "^0.4.0",
    "vite-plugin-dts": "^3.9.0"
  },
  "dependencies": {
    "@incubateur-ademe/legal-pages-markdown": "0.0.3-next.1",
    "mustache": "^4.2.0"
  }
}
